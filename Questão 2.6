package main

import (
	"fmt"
)

func main() {
	var (
		A, B int64
	)

	fmt.Println("Digite um valor: ")
	fmt.Scanln(A)

	fmt.Println("Digite outro valor: ")
	fmt.Scanln(B)

	if A%B == 0 {
		fmt.Println("O número", A, "é dívisivel por", B)
	} else {
		fmt.Println("O número", A, "não é dívisivel por", B)
	}
}
